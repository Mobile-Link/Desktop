<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vmNoAuth="clr-namespace:MobileLink_Desktop.ViewModels.NoAuth"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MobileLink_Desktop.Views.NoAuth.LoginCreateDevice"
             x:DataType="vmNoAuth:LoginCreateDeviceViewModel">
    <StackPanel>
        <Button Content="Voltar" Command="{Binding GoBack}" />
        <TextBlock Text="Nomeie seu dispositivo" />
        <Label Content="Nome do Dispositivo" />
        <TextBox Text="{Binding DeviceName}" />
        <Button Content="Criar dispositivo" Command="{Binding CreateDevice}" />
    </StackPanel>
</UserControl>